print("Fizz Buzz Program:")
n = int(input("Enter the number: "))

# Iterate through numbers from 1 to n
for i in range(1, n + 1):
    if i % 3 == 0 and i % 5 == 0:
        print(f"{i} = Fizz Buzz")
    elif i % 3 == 0:
        print(f"{i} = Fizz")
    elif i % 5 == 0:
        print(f"{i} = Buzz")
    else:
        print(i)
